{
    "name": "@internal/plugins",
    "version": "0.0.1",
    "license": "MIT",
    "private": true,
    "sideEffects": false,
    "type": "module",
    "main": "./dist/esm/index.js",
    "export": "./dist/esm/index.js",
    "scripts": {
        "build": "rm -fr dist/* && tsc -p ./tsconfig.json && tsc -p ./tsconfig-cjs.json",
        "rename": "mv ./dist/index.js ./dist/index.cjs"
    },
    "dependencies": {
        "@mapbox/rehype-prism": "^0.8.0",
        "@mdx-js/loader": "^2.0.0-rc.2",
        "@mdx-js/rollup": "^2.0.0-rc.2",
        "@types/mdx": "^2.0.1",
        "@types/node": "^14.12.20",
        "gray-matter": "^4.0.3",
        "remark-frontmatter": "^4.0.1",
        "remark-gfm": "^1.0.0",
        "remark-mdx-frontmatter": "^1.1.1",
        "remark-unwrap-images": "^3.0.1",
        "typescript": "^4.2.4",
        "vite": "^2.7.0"
    },
    "xjest": {
        "collectCoverageFrom": [
            "**/*.{ts,tsx}",
            "!**/examples.tsx",
            "!**/node_modules/**"
        ]
    },
    "browserslist": {
        "production": [
            ">0.2%",
            "not dead",
            "not op_mini all"
        ],
        "development": [
            "last 1 chrome version",
            "last 1 firefox version",
            "last 1 safari version"
        ]
    },
    "eslintConfig": {
        "extends": [
            "react-app"
        ],
        "overrides": [
            {
                "files": [
                    "**/*.ts?(x)"
                ],
                "rules": {
                    "import/no-webpack-loader-syntax": "off"
                }
            }
        ]
    }
}
