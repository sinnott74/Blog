{
    "name": "client",
    "version": "0.0.1",
    "license": "MIT",
    "private": true,
    "sideEffects": false,
    "scripts": {
        "build": "tsc && vite build",
        "start": "vite",
        "serve": "vite preview",
        "storybook": "NODE_PATH=$(pwd) start-storybook -p 9001 --no-manager-cache",
        "storybook-build": "build-storybook",
        "test": "NODE_OPTIONS=--experimental-vm-modules jest --coverage"
    },
    "dependencies": {
        "@mdx-js/react": "^2.0.0-next.9",
        "deepmerge": "4.2.2",
        "lodash.clonedeep": "^4.5.0",
        "material-colors": "^1.2.6",
        "material-design-icons": "^3.0.1",
        "prism-react-renderer": "^1.2.1",
        "react": "^17.0.2",
        "react-dom": "^17.0.2",
        "react-helmet": "^6.1.0",
        "react-intersection-observer": "^8.26.1",
        "react-router-dom": "^6.0.2",
        "react-transition-group": "^4.3.0",
        "styled-components": "^5.3.0"
    },
    "devDependencies": {
        "@mdx-js/loader": "^2.0.0-next.9",
        "@storybook/addon-a11y": "^6.3.4",
        "@storybook/addon-actions": "^6.3.4",
        "@storybook/addon-docs": "^6.3.4",
        "@storybook/addon-essentials": "^6.3.4",
        "@storybook/addon-storysource": "^6.3.4",
        "@storybook/addon-viewport": "^6.3.4",
        "@storybook/react": "^6.3.4",
        "@testing-library/jest-dom": "^5.12.0",
        "@testing-library/react": "^11.2.6",
        "@types/enzyme": "^3.10.5",
        "@types/jest": "^26.0.24",
        "@types/lodash.clonedeep": "^4.5.6",
        "@types/node": "^14.12.20",
        "@types/prismjs": "~1.16.0",
        "@types/react": "^17.0.3",
        "@types/react-dom": "^17.0.3",
        "@types/react-helmet": "^6.1.1",
        "@types/react-transition-group": "^4.2.4",
        "@types/styled-components": "^5.1.9",
        "@vitejs/plugin-react-refresh": "^1.3.5",
        "apollo-storybook-react": "^0.2.3",
        "babel-plugin-macros": "^3.1.0",
        "gray-matter": "^4.0.3",
        "remark-frontmatter": "^3.0.0",
        "remark-gfm": "^1.0.0",
        "remark-mdx-frontmatter": "^1.0.1",
        "remark-unwrap-images": "^2.1.0",
        "rollup-plugin-analyzer": "^4.0.0",
        "storybook-builder-vite": "^0.0.12",
        "storybook-react-router": "^1.0.8",
        "typescript": "^4.2.4",
        "vite": "^2.4.2",
        "vite-plugin-babel-macros": "^1.0.5",
        "vite-plugin-mdx": "^3.5.6",
        "vite-plugin-svgr": "^0.3.0"
    },
    "xjest": {
        "collectCoverageFrom": [
            "**/*.{ts,tsx}",
            "!**/examples.tsx",
            "!**/node_modules/**"
        ]
    },
    "browserslist": {
        "production": [
            ">0.2%",
            "not dead",
            "not op_mini all"
        ],
        "development": [
            "last 1 chrome version",
            "last 1 firefox version",
            "last 1 safari version"
        ]
    },
    "eslintConfig": {
        "extends": [
            "react-app"
        ],
        "overrides": [
            {
                "files": [
                    "**/*.ts?(x)"
                ],
                "rules": {
                    "import/no-webpack-loader-syntax": "off"
                }
            }
        ]
    }
}
